{
    "agent": {
      "metrics_collection_interval": 60,
      "run_as_user": "root"
    },
    "metrics": {
      "namespace": "CWAgent",
      "append_dimensions": {
        "InstanceId": "${aws:InstanceId}"
      },
      "metrics_collected": {
        "cpu": {
          "measurement": [
            "cpu_usage_idle",
            "cpu_usage_iowait",
            "cpu_usage_system",
            "cpu_usage_user"
          ],
          "metrics_collection_interval": 60,
          "totalcpu": true
        },
        "mem": {
          "measurement": [
            "mem_used_percent"
          ],
          "metrics_collection_interval": 60
        }
      }
    }
  }
  